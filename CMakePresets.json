{
    "version": 8,
    "configurePresets": [
        {
            "name": "gcc",
            "displayName": "GCC 12.3.0 x86_64-linux-gnu",
            "description": "Using compilers: C = /usr/bin/gcc-12",
            "binaryDir": "${sourceDir}/build/${presetName}",
            "cacheVariables": {
                "CMAKE_C_COMPILER": "/usr/bin/gcc-12",
                "CMAKE_BUILD_TYPE": "Debug"
            }
        }
    ]
}